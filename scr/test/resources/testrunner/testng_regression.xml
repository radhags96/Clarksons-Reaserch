<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Clarksons Research Test Regression Suite" thread-count="3"
	parallel="tests" verbose="4">


	<listeners>
		<listener
			class-name="com.qa.clarksons.listeners.Listeners" />

	</listeners>
	<test name="Sign Up Page Test_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.clarksons.tests.SignUpPageTest" />
		</classes>
	</test>
	
